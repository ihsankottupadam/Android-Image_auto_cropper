<?xml version ="1.0" encoding ="utf-8" ?>
<layer-list xmlns:android ="http://schemas.android.com/apk/res/android" >
	<item android:id ="@android:id/background" >
		<shape android:shape="rectangle">
			<padding android:left= "2dp" android:top= "2dp" android:right= "2dp" android:bottom= "2dp" />
			<solid android:color="#22ffffff"/>
			<corners android:topLeftRadius="1dp" android:topRightRadius="1.0dp" android:bottomRightRadius="1.0dp" android:bottomLeftRadius="1.0dp"/>
			<stroke android:width=".2dp" android:color="#44ffffff"/>
		</shape>
	</item>
	<item android:id ="@android:id/secondaryProgress" >
		<clip>
			<shape >
				<corners
					android:radius ="5dip" />
				<gradient
					android:angle ="270"
					android:centerColor = "#80ffb600"
					android:centerY = "0.75"
					android:endColor ="#a0ffcb00"
					android:startColor ="#80ffd300" />
			</shape>
		</clip>
	</item>
	<item android:id ="@android:id/progress" >
		<clip>
			<shape android:shape="rectangle">
				<padding android:left= "2dp" android:top= "2dp" android:right= "2dp" android:bottom= "2dp" />
				<gradient android:type="linear" android:angle="90" android:startColor="#046AD2" android:centerColor="#2371BD" android:endColor="#97C2EA"/>
				<corners android:topLeftRadius="1.0dp" android:topRightRadius="1.0dp" android:bottomRightRadius="1.0dp" android:bottomLeftRadius="1.0dp"/>
				<stroke android:width=".2dp" android:color="#00ffffff"/>
			</shape>
		</clip>
	</item>
</layer-list >
